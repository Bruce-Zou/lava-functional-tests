{
  "device_type": "arndale",
  "logging_level": "DEBUG",
  "actions": [
    {
        "command": "deploy_linaro_android_image",
        "parameters": {
            "boot": "http://images.validation.linaro.org/functional-test-images/arndale/boot.tar.bz2",
            "data": "http://images.validation.linaro.org/functional-test-images/arndale/userdata.tar.bz2",
            "system": "http://images.validation.linaro.org/functional-test-images/arndale/system.tar.bz2"
        }
    }, 
    {
      "command": "boot_linaro_android_image"
    },
    {
        "command": "lava_android_test_install",
        "parameters": {
            "tests": [
                "busybox"
            ]
        }
    },
    {
        "command": "lava_android_test_run",
        "parameters": {
            "test_name": "busybox"
        }
    },   
    {
      "command": "submit_results",
      "parameters": {
        "stream": "BUNDLE_STREAM",
        "server": "LAVA_SERVER"
      }
    }
  ],
  "timeout": 18000,
  "job_name": "master-boot-options-boot-cmds-lava-android-test-android"
}
