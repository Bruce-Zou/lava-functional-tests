{
  "timeout": 18000,
  "job_name": "kvm-kernel-lava-test-shell",
  "device_type": "kvm",
  "logging_level": "DEBUG",
  "actions": [
    {
      "command": "deploy_linaro_kernel",
      "parameters": {
        "kernel": "http://community.validation.linaro.org/images/kvm/vmlinuz-3.5.0-17-generic",
        "ramdisk": "http://community.validation.linaro.org/images/kvm/initrd.img-3.5.0-17-generic",
        "rootfs": "http://community.validation.linaro.org/images/kvm/kvm-image.img.gz"
      }
    },
    {
      "command": "lava_test_shell",
      "parameters": {
        "testdef_repos": [
          {
            "git-repo": "git://git.linaro.org/qa/test-definitions.git",
            "testdef": "ubuntu/dmidecode.yaml"
          }
        ],
        "timeout": 9600
      }
    },
    {
      "command": "submit_results",
      "parameters": {
        "stream": "BUNDLE_STREAM",
        "server": "LAVA_SERVER"
      }
    }
  ]
}
