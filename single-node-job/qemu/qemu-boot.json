{
  "timeout": 18000,
  "job_name": "qemu-boot",
  "device_type": "qemu",
  "actions": [
    {
      "command": "deploy_linaro_image",
      "parameters": {
        "image": "http://community.validation.linaro.org/images/qemu/beagle-nano.img.gz"
        }
    },
    {
      "command": "boot_linaro_image"
    },
    {
      "command": "submit_results",
      "parameters": {
        "stream": "BUNDLE_STREAM",
        "server": "LAVA_SERVER"
      }
    }
  ]
}
