{
    "actions": [
        {
            "command": "deploy_linaro_android_image",
            "parameters": {
                "boot": "http://images.validation.linaro.org/functional-test-images/ifc6410/linaro-boot.img",
                "system": "http://images.validation.linaro.org/functional-test-images/ifc6410/firmware-ifc6410.img",
                "data": "http://images.validation.linaro.org/functional-test-images/ifc6410/linaro-trusty-developer-ifc6410.img.gz",
                "target_type": "ubuntu"
            }
        },
        {
            "command": "boot_linaro_image"
        },
        {
            "command": "submit_results",
            "parameters": {
                "stream": "BUNDLE_STREAM",
                "server": "LAVA_SERVER"
            }
        }
    ],
    "device_type": "ifc6410",
    "job_name": "ifc6410-image-boot",
    "timeout": 18000
}
