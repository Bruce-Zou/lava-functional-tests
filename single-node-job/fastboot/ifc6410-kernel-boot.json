{
    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "parameters": {
                "kernel": "http://community.validation.linaro.org/images/ifc6410/zImage_ifc6410",
                "rootfs": "http://community.validation.linaro.org/images/ifc6410/linaro-saucy-developer-ifc6410.img.gz",
                "target_type": "ubuntu"
            }
        },
        {
            "command": "boot_linaro_image"
        },
        {
            "command": "submit_results",
            "parameters": {
                "stream": "BUNDLE_STREAM",
                "server": "LAVA_SERVER"
            }
        }
    ],
    "device_type": "ifc6410",
    "job_name": "ifc6410-kernel-boot",
    "timeout": 18000
}
