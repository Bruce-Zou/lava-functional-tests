{
  "device_type": "wg",
  "logging_level": "DEBUG",
  "actions": [
    {
            "command": "deploy_linaro_image",
            "parameters": {
                "image": "https://releases.linaro.org/14.05/openembedded/aarch64/vexpress64-openembedded_minimal-armv8-gcc-4.8_20140530-656.img.gz",
                "bootloadertype": "uefi"
            }
    },
    {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_repos": [
                    {
                        "git-repo": "git://git.linaro.org/qa/test-definitions.git",
                        "testdef": "openembedded/smoke-tests-basic.yaml"
                    }
                ]
            }
    },
    {
      "command": "submit_results",
      "parameters": {
        "stream": "/private/team/wg/wg-private/",
        "server": "LAVA_SERVER"
      }
    }
  ],
  "timeout": 18000,
  "job_name": "wg-oe-lava-test-shell"
}
