{
  "device_type": "wg",
  "logging_level": "DEBUG",
  "actions": [
    {
            "command": "deploy_linaro_android_image",
            "parameters": {
                "boot": "https://releases.linaro.org/14.05/android/images/juno/boot.tar.bz2",
                "bootloadertype": "uefi",
                "data": "https://releases.linaro.org/14.05/android/images/juno/userdata.tar.bz2",
                "system": "https://releases.linaro.org/14.05/android/images/juno/system.tar.bz2"
            }
    },
    {
            "command": "boot_linaro_android_image"
    },
    {
        "command": "lava_android_test_install",
        "parameters": {
            "tests": [
                "shell-custom"
            ]
        }
    },
    {
        "command": "lava_android_test_run",
        "parameters": {
            "option": "logcat",
            "test_name": "shell-custom"
        }
    },
    {
      "command": "submit_results",
      "parameters": {
        "stream": "/private/team/wg/wg-private/",
        "server": "LAVA_SERVER"
      }
    }
  ],
  "timeout": 18000,
  "job_name": "wg-android-lava-android-test"
}
