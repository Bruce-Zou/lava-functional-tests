{
    "actions": [
        {
            "command": "deploy_linaro_android_image",
            "metadata": {
                "android.build": "340",
                "android.name": "linaro-android_armv8-aosp-master-juno-lsk-stlport",
                "android.url": "http://people.linaro.org/~zhizhou.tian/images/juno-64/"
            },
            "parameters": {
                "bootloadertype": "uefi",
                "images": [
                    {
                        "partition": "boot",
                        "url": "http://images.validation.linaro.org/functional-test-images/juno/boot.tar.bz2"
                    },
                    {
                        "partition": "userdata",
                        "url": "http://images.validation.linaro.org/functional-test-images/juno/userdata.tar.bz2"
                    },
                    {
                        "partition": "system",
                        "url": "http://images.validation.linaro.org/functional-test-images/juno/system.tar.bz2"
                    }
                ]
            }
        },
        {
            "command": "boot_linaro_android_image",
            "parameters": {
                "enable_network_after_boot_android": false
            }
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "lava_test_dir": "/data/local/tmp/lava",
                "lava_test_results_dir": "/local/tmp/lava",
                "testdef_repos": [
                    {
                        "git-repo": "git://git.linaro.org/qa/test-definitions.git",
                        "testdef": "android/busybox.yaml"
                    }
                ],
                "timeout": 1800
            }
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "lava_test_dir": "/data/local/tmp/lava",
                "lava_test_results_dir": "/local/tmp/lava",
                "testdef_repos": [
                    {
                        "git-repo": "git://git.linaro.org/qa/test-definitions.git",
                        "testdef": "android/boottime.yaml"
                    }
                ],
                "timeout": 1800
            }
        }
    ],
    "device_type": "juno",
    "image_type": "android",
    "job_name": "linaro-android_armv8-aosp-master-juno-lsk-stlport",
    "timeout": 72000
}
