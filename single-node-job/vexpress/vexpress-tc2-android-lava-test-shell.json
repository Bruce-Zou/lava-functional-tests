{
    "actions": [
        {
            "command": "deploy_linaro_android_image",
            "parameters": {
                "boot": "http://releases.linaro.org/14.03/android/images/vexpress-linaro/boot.tar.bz2",
                "data": "http://releases.linaro.org/14.03/android/images/vexpress-linaro/userdata.tar.bz2",
                "system": "http://releases.linaro.org/14.03/android/images/vexpress-linaro/system.tar.bz2"
            }
        },
        {
            "command": "boot_linaro_android_image"
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_repos": [
                    {
                        "git-repo": "git://git.linaro.org/qa/test-definitions.git",
                        "testdef": "common/pm-qa.yaml"
                    }
                ]
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "LAVA_SERVER",
                "stream": "BUNDLE_STREAM"
            }
        }
    ],
    "device_type": "vexpress-tc2",
    "image_type": "android",
    "job_name": "vexpress-tc2-android-lava-test-shell",
    "timeout": 18000
}
