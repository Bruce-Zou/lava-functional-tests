{
    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "parameters": {
                "bl1": "http://images.validation.linaro.org/functional-test-images/armv8-fvp/fvp_bl1.bin",
                "bootloader": "http://images.validation.linaro.org/functional-test-images/armv8-fvp/fvp_fip.bin",
                "bootloadertype": "uefi",
                "dtb": "http://images.validation.linaro.org/functional-test-images/armv8-fvp/fvp-base-gicv2-psci.dtb",
                "kernel": "http://images.validation.linaro.org/functional-test-images/armv8-fvp/Image",
                "ramdisk": "http://images.validation.linaro.org/functional-test-images/common/linaro-image-minimal-initramfs-genericarmv8-20140605-682.rootfs.cpio.gz"
            }
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_repos": [
                    {
                        "git-repo": "git://git.linaro.org/qa/test-definitions.git",
                        "testdef": "openembedded/busybox.yaml"
                    }
                ]
            }
        },
        {
          "command": "submit_results",
          "parameters": {
            "stream": "BUNDLE_STREAM",
            "server": "LAVA_SERVER"
          }
        }
    ],
    "device_type": "rtsm_fvp_base-aemv8a",
    "job_name": "fastmodel-armv8-fvp-base-aemv8a-kernel-boot-lava-test-shell",
    "logging_level": "DEBUG",
    "timeout": 20000
}
