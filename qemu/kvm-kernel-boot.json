{
  "timeout": 18000,
  "job_name": "kvm-kernel-boot",
  "device_type": "kvm",
  "logging_level": "DEBUG",
  "actions": [
    {
      "command": "deploy_linaro_kernel",
      "parameters": {
        "kernel": "http://community.validation.linaro.org/images/kvm/vmlinuz-3.5.0-17-generic",
        "ramdisk": "http://community.validation.linaro.org/images/kvm/initrd.img-3.5.0-17-generic",
        "rootfs": "file:///srv/lava/instances/production/var/www/lava-server/images/kvm/test_image.img"
      }
    },
    {
      "command": "boot_linaro_image"
    },
    {
      "command": "submit_results",
      "parameters": {
        "stream": "BUNDLE_STREAM",
        "server": "LAVA_SERVER"
      }
    }
  ]
}
